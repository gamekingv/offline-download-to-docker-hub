{
  "env": {
    "node": true,
    "webextensions": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "rules": {
    "semi": "warn",
    "quotes": [
      "warn",
      "single"
    ],
    "no-var": "error",
    "no-unused-vars": "warn"
  }
}